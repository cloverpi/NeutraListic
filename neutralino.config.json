{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/ui/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "filesystem.readDirectory"
  ],
  "modes": {
    "window": {
      "title": "NeutraListic",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/ui/public/logo192.png",
      "enableInspector": true
    }
  },
  "cli": {
    "binaryName": "NeutraListic",
    "extensionsPath": "/extensions/",
    "resourcesPath": "/ui/build/",
    "frontendLibrary": {
      "patchFile": "/ui/public/index.html",
      "devUrl": "http://localhost:3000",
      "projectPath": "/ui/",
      "initCommand": "npm install",
      "devCommand": "npm run start",
      "buildCommand": "npm run build",
      "waitTimeout": 20000
    },
    "binaryVersion": "6.3.0"
  }
}